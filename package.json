    "@typescript-eslint/parser": "^6.2.1",
    "autoprefixer": "^10.4.13",
    "encoding": "^0.1.13",
    "eslint": "^8.35.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "typescript": "5.1.3"
  }
